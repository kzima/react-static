{
  "name": "reactStatic",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "pushstate-server": "^1.14.0",
    "react-router": "^2.7.0",
    "react-static-generator": "0.1.0",
    "surge": "^0.18.0",
    "react-scripts": "0.4.1"
  },
  "dependencies": {
    "react": "^15.3.1",
    "react-dom": "^15.3.1"
